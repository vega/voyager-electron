{
  "name": "voyager-electron",
  "version": "0.0.1",
  "description": "A minimal Electron application that embeds voyager",
  "main": "main.js",
  "scripts": {
    "build": "webpack --config=webpack.config.js",
    "start": "electron .",
    "start:server": "electron . --server",
    "dist": "electron-builder",
    "dist-all": "electron-builder -mwl"
  },
  "build": {
    "appId": "vega.github.io/voyager2",
    "mac": {
      "target": "dmg"
    },
    "linux": {
      "target": "tar.gz"
    },
    "win": {
      "target": "zip"
    }
  },
  "repository": "",
  "keywords": [],
  "contributors": [
    "Yannick Assogba <yannick@bocoup.com>"
  ],
  "license": "BSD-3-Clause",
  "dependencies": {
    "d3-dsv": "^1.0.5",
    "datavoyager": "../voyager",
    "datavoyager-server": "../voyager-server",
    "electron-debug": "^1.1.0",
    "font-awesome": "4.6.3",
    "font-awesome-sass-loader": "1.0.3",
    "jschardet": "^1.4.2",
    "react-css-modules": "4.1.0",
    "react-tether": "0.5.6",
    "redux-undo": "1.0.0-beta9-9-6",
    "vega": "3.0.0-rc2",
    "vega-datasets": "^1.8.0",
    "vega-lite": "2.0.0-rc1",
    "vega-tooltip": "^0.4.2",
    "whatwg-fetch": "https://registry.npmjs.org/whatwg-fetch/-/whatwg-fetch-1.0.0.tgz"
  },
  "devDependencies": {
    "electron": "~1.6.10",
    "css-loader": "^0.23.1",
    "electron-builder": "^19.16.2",
    "eslint": "^3.19.0",
    "eslint-config-airbnb-base": "^11.2.0",
    "eslint-plugin-import": "^2.3.0",
    "file-loader": "^0.11.2",
    "node-loader": "^0.6.0",
    "node-sass": "^4.5.3",
    "sass-loader": "4.1.1",
    "style-loader": "0.13.1",
    "url-loader": "^0.5.9",
    "webpack": "^3.5.5"
  }
}
