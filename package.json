{
  "name": "voyager-electron",
  "version": "0.0.1",
  "description": "A minimal Electron application that embeds voyager",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "dist": "electron-builder",
    "dist-all": "electron-builder -mwl"
  },
  "build": {
    "appId": "vega.github.io/voyager2",
    "mac": {
      "target": "dmg"
    },
    "linux": {
      "target": "tar.gz"
    },
    "win": {
      "target": "zip"
    }
  },
  "repository": "",
  "keywords": [],
  "contributors": [
    "Yannick Assogba <yannick@bocoup.com>"
  ],
  "license": "BSD-3-Clause",
  "dependencies": {
    "d3-dsv": "^1.0.5",
    "datavoyager": "^2.0.0-alpha.8",
    "electron-debug": "^1.1.0",
    "jschardet": "^1.4.2"
  },
  "devDependencies": {
    "electron": "~1.6.10",
    "electron-builder": "^19.16.2",
    "eslint": "^3.19.0",
    "eslint-config-airbnb-base": "^11.2.0",
    "eslint-plugin-import": "^2.3.0"
  }
}
